{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [
            "lib",
            "step-definitions",
            "README.md"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "default": {
            "outputSourceFiles": false
        },
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./docs/",
        "template": "node_modules/docdash",
        "recurse": true,
        "verbose": true
    },
    "docdash": {
        "static": true,
        "sort": false,
        "sectionOrder": [
            "Classes",
            "Modules",
            "Externals",
            "Events",
            "Namespaces",
            "Mixins",
            "Tutorials",
            "Interfaces"
        ],
        "disqus": "",
        "openGraph": {
            "title": "",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "",
            "description": "",
            "keyword": ""
        },
        "search": true,
        "commonNav": true,
        "collapse": true,
        "wrap": true,
        "typedefs": true,
        "navLevel": 1,
        "private": true,
        "removeQuotes": "none",
        "scripts": [],
        "menu": {
            "Project Website": {
                "href": "www.test.com",
                "target": "_blank",
                "class": "menu-item",
                "id": "website_link"
            },
            "Forum": {
                "href": "",
                "target": "_blank",
                "class": "menu-item",
                "id": "forum_link"
            }
        },
        "scopeInOutputPath": true,
        "nameInOutputPath": true,
        "versionInOutputPath": true
    }
}